<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="">
    <meta name="author" content="Dmitry DarkAiR Romanov">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--meta viewport="width=device-width"-->

    <link rel="shortcut icon" href="{{ App.baseUrl }}/img/icons/favicon.ico">
    <link rel="icon" type="image/png" href="{{ App.baseUrl }}/img/icons/favicon.png">
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="{{ App.baseUrl }}/img/icons/icon152.png">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="{{ App.baseUrl }}/img/icons/icon144.png">
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="{{ App.baseUrl }}/img/icons/icon120.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="{{ App.baseUrl }}/img/icons/icon114.png">   {# Иконка для iPhone #}
    <link rel="apple-touch-icon-precomposed" sizes="72x72"   href="{{ App.baseUrl }}/img/icons/icon72.png">    {# Иконка для iPad #}
    <link rel="apple-touch-icon-precomposed" href="{{ App.baseUrl }}/img/icons/icon57.png">                    {# Обычная иконка 57x57 #}

    <meta http-equiv="cleartype" content="on">                  {# Принудительное сглаживание шрифтов для Mobile IE #}
    <meta name="format-detection" content="telephone=no">       {# Отключаем распознавание телефона #}
    <meta name="format-detection" content="address=no">         {# Отключаем распознавание адреса #}

    <link rel="canonical" href="{{ this.canonicalUrl }}">

    <title>{% block title %}{{ this.pageTitle }}{% endblock %}</title>

    {% block js %}
        {{ import('js', 'bootstrap.js') }}
        {{ import('js', 'ace.js', 'ext.mAdmin.assets.js') }}
    {% endblock %}

    {% block css %}
        {{ import('css', 'bootstrap.css') }}
        {{ import('css', 'ace.css') }}
        {{ import('css', 'custom.css') }}
    {% endblock %}
</head>
<body {% block bodyAttr %}{% endblock %}>
    {% block bodyStart %}
    {% endblock %}

    <div class='hidden-print'>
        {% block content %}
        {% endblock %}
    </div>

    <div class='visible-print'>
        {% block print %}
        {% endblock %}
    </div>

    {% block bodyEnd %}
    {% endblock %}
</body>
</html>
